{
  "name": "surface",
  "description": "Lift front-end boilerplate.",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/l1f7/surface"
  },
  "scripts": {
    "start": "npm run lint:versions -s && npm run watch -s",
    "lint:versions": "NVM_VERSION=$(cat .nvmrc); NODE_VERSION=$(node --version); echo $NODE_VERSION | grep -q $NVM_VERSION && echo \"✓ running node $NODE_VERSION - all clear\" || (echo \"✗ running node $NODE_VERSION - expected $NVM_VERSION. Use nvm to install the right version.\" && exit 1)",
    "proto": "NODE_ENV=prototype gulp build",
    "dev": "NODE_ENV=development gulp build",
    "prod": "NODE_ENV=production gulp build",
    "watch": "gulp watch",
    "lock": "npm run lint:versions -s && npm shrinkwrap --dev && sed -i '' 's/https:/http:/g' npm-shrinkwrap.json"
  },
  "dependencies": {
    "jquery": "^2.2.3",
    "normalize-scss": "^6.0.0",
    "throttle-debounce": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-latest": "^6.22.0",
    "browser-sync": "^2.18.8",
    "del": "^2.2.2",
    "es6-promise": "^4.0.5",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "expose-loader": "^0.7.3",
    "gaze": "^1.1.2",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-cached": "^1.1.1",
    "gulp-changed": "^2.0.0",
    "gulp-cssmin": "0.1.7",
    "gulp-debug": "^3.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-filter": "^5.0.0",
    "gulp-flatten": "^0.3.1",
    "gulp-html2pug": "^0.0.2",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.3.0",
    "gulp-print": "^2.0.1",
    "gulp-pug": "^3.2.0",
    "gulp-pug-inheritance": "^0.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sass-lint": "^1.3.2",
    "gulp-sequence": "^0.4.6",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-svgmin": "^1.2.3",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "lodash": "^4.17.4",
    "node-notifier": "^5.0.2",
    "object-assign": "^4.1.1",
    "require-dir": "^0.3.1",
    "respawn": "^2.5.0",
    "stylelint": "^7.9.0",
    "stylelint-config-recess-order": "^1.0.0",
    "stylelint-order": "^0.3.0",
    "stylelint-scss": "^1.4.3",
    "text-table": "^0.2.0",
    "vinyl": "^2.0.1",
    "webpack": "^2.2.1",
    "webpack-merge": "^3.0.0",
    "yargs": "^7.0.0"
  }
}
